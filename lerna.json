{
  "useWorkspaces": true,
  "version": "0.0.5",
  "npmClient": "yarn",
  "command": {
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "exact": true,
      "ignoreChanges": [
        "**/*.md"
      ],
      "message": "build: release version %v"
    },
    "bootstrap": {
      "ignore": "npm-*",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  }
}